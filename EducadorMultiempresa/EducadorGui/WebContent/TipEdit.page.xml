<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/TipList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{tipHome.wire}"/>

   <param name="tipFrom"/>
   <param name="tipIdTip" value="#{tipHome.tipIdTip}"/>
   <param name="moduloFrom"/>
   <param name="moduloIdModulo" value="#{moduloHome.moduloIdModulo}"/>
   <param name="proyectoFrom"/>
   <param name="proyectoIdProyecto" value="#{proyectoHome.proyectoIdProyecto}"/>




   <navigation from-action="#{tipHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/Tip.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{tipHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/Tip.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{tipHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/TipList.xhtml"/>
      </rule>
   </navigation>

</page>
