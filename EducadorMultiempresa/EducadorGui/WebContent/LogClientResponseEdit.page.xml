<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/LogClientResponseList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{logClientResponseHome.wire}"/>

   <param name="logClientResponseFrom"/>
   <param name="logClientResponseIdLogClientResponse" value="#{logClientResponseHome.logClientResponseIdLogClientResponse}"/>
   <param name="suscriptorFrom"/>
   <param name="suscriptorIdSuscriptor" value="#{suscriptorHome.suscriptorIdSuscriptor}"/>



   <navigation from-action="#{logClientResponseHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/LogClientResponse.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{logClientResponseHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/LogClientResponse.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{logClientResponseHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/LogClientResponseList.xhtml"/>
      </rule>
   </navigation>

</page>
