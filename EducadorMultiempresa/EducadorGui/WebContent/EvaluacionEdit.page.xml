<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/EvaluacionList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{evaluacionHome.wire}"/>

   <param name="evaluacionFrom"/>
   <param name="evaluacionIdEvaluacion" value="#{evaluacionHome.evaluacionIdEvaluacion}"/>
   <param name="moduloFrom"/>
   <param name="moduloIdModulo" value="#{moduloHome.moduloIdModulo}"/>
   <param name="proyectoFrom"/>
   <param name="proyectoIdProyecto" value="#{proyectoHome.proyectoIdProyecto}"/>




   <navigation from-action="#{evaluacionHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/Evaluacion.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{evaluacionHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/Evaluacion.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{evaluacionHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/EvaluacionList.xhtml"/>
      </rule>
   </navigation>

</page>
