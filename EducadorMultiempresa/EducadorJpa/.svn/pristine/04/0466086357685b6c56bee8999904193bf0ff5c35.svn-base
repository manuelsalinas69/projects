		<class>py.com.global.educador.jpa.entity.EjecucionSuscriptorDetalle</class>
		<class>py.com.global.educador.jpa.entity.Evaluacion</class>
		<class>py.com.global.educador.jpa.entity.LogClientResponse</class>
		<class>py.com.global.educador.jpa.entity.LogSmsIn</class>
		<class>py.com.global.educador.jpa.entity.LogSmsOut</class>
		<class>py.com.global.educador.jpa.entity.LogSuscripcion</class>
		<class>py.com.global.educador.jpa.entity.Modulo</class>
		<class>py.com.global.educador.jpa.entity.PlanificacionEnvio</class>
		<class>py.com.global.educador.jpa.entity.Pregunta</class>
		<class>py.com.global.educador.jpa.entity.Proyecto</class>
		<class>py.com.global.educador.jpa.entity.Respuesta</class>
		<class>py.com.global.educador.jpa.entity.SessionTrx</class>
		<class>py.com.global.educador.jpa.entity.Suscriptor</class>
		<class>py.com.global.educador.jpa.entity.SuscriptorProyecto</class>
		<class>py.com.global.educador.jpa.entity.SuscriptorProyectoPK</class>
		<class>py.com.global.educador.jpa.entity.Tip</class>
		<class>py.com.global.educador.jpa.entity.EvaluacionSuscriptor</class>
		<class>py.com.global.educador.jpa.entity.SuscriptorModulo</class>
		
		
	
	<properties>
         <property name="hibernate.dialect" value="org.hibernate.dialect.Oracle10gDialect"/>
         <property name="hibernate.hbm2ddl.auto" value="none"/>
<!--          <property name="hibernate.show_sql" value="true"/> -->
         <property name="hibernate.show_sql" value="none"/>
         <property name="hibernate.format_sql" value="true"/>
         <property name="hibernate.id.new_generator_mappings" value="false"/>
<!--          <property name="jboss.entity.manager.factory.jndi.name" value="java:/SAPAEntityManagerFactory"/> -->
      </properties>
     </persistence-unit>
</persistence>
